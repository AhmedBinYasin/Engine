
public class Main {

	public static void main(String[] args) {
		Date d1 = new Date (21,6,2020);
		Date d2 = new Date (31,8,2021);
		System.out.println("The date difference between "+d1.print()+" and "+d2.print()+" is "+
		d1.dayDiff(d1, d2)+" days.");
		System.out.print("The date after increment of 400 days in "+d2.print()+" is ");
		d2.dayIncrement(400);
		System.out.println(d2.print()+".");
		
	}

}
