public class assign12 {
    private int dd1,mm1,yy1,dd2,mm2,yy2;
		int month,day;
		int date;
               
		public int date(int dd1,int dd2,int mm1,int mm2,int yy1,int yy2) {
		if(0<dd1 &&  dd1<=31 && 0<mm1 && mm1<=12){
           System.out.println("DATE 1 ENTERED IS "+""+dd1+"-"+""+mm1+"-"+""+yy1);
            }
        else{
            System.out.println("INCORRECT DATE ENTERED");
        }
                if(0<dd2 && dd2<=31  && 0<mm2 && mm2<=12){ 
             System.out.println("DATE 2 ENTERED IS "+""+dd2+"-"+""+mm2+"-"+""+yy2);
           
        }
        else{
            System.out.println("INCORRECT DATE ENTERED");
        }  
        return 0;
                }
             public int date1( ) {
			mm1=1;
			dd1=0;
			while( dd1!=0 ){
				if( mm1%2!=0 ){
					if( dd1==31 ) {
						dd1=1;
						mm1++;
					}
					else{
						dd1++;
					}
				}
				else if ( mm1==2 ) {
					if( dd1==29 ) {
						dd1=1;
						mm1++;
					}
					else{
						dd1++;
					}
				}
				else{
					if( dd1==30 ) {
						dd1=1;
						mm1++;
					}
					else{
						dd1++;
					}
				}
				dd1--;
			}
        return 0;
		}
             public int date2(){
                 mm2=1;
			dd2=0;
			while( dd2!=0 ){
				if( mm2%2!=0 ){
					if( dd2==31 ) {
						dd2=1;
						mm2++;
					}
					else{
						dd2++;
					}
				}
				else if ( mm2==2 ) {
					if( dd2==29 ) {
						dd2=1;
						mm2++;
					}
					else{
						dd1++;
					}
				}
				else{
					if( dd2==30 ) {
						dd2=1;
						mm2++;
					}
					else{
						dd2++;
					}
				}
				dd2--;
			}
        return 0;
             }
              
                     int  x=dd2-dd1;
                   public int getdiff1(){
                    return x;
                }
                    public int setdiff1(int dif1){
                        dif1=x;
                        return dif1;
                    }
                      int  y=mm2-mm1;
                     public int getdiff2(){
                    return y;
                }
                    public int setdiff2(int diff2){
                        diff2=y;
                        return diff2;
                    }
                      int  z=yy1-yy2;
                     public int getdiff3(){
                    return z;
                }
                    public int setdiff3(int diff3){
                        diff3=z;
                        return diff3;
                    }
                    
                    
